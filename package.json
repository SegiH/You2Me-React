{
  "name": "y2m-noserver",
  "homepage": "https://segi.mooo.com/YouTube2MP3/",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/php": {
      "target": "https://segi.mooo.com/Y2M/",
      "changeOrigin": true
    }
  },
  "dependencies": {
    "@angular/core": "^5.0.5",
    "@material-ui/core": "^3.0.3",
    "@material-ui/icons": "^3.0.1",
    "dns": "^0.2.2",
    "fetch": "^1.1.0",
    "mobile-detect": "^1.3.7",
    "muicss": "^0.9.35",
    "nativescript-screen-orientation": "^2.0.0",
    "react": "^16.5.0",
    "react-css-modules": "^4.7.1",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-scripts": "^1.1.5",
    "redux": "^3.7.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy-standalone": "cp -R src/php build && cp -R src/python build && cp -R build/* /var/www/html/YouTube2MP3/",
    "deploy-server": "cp -R src/php build && cp -R src/python build && cp -R build/* /var/www/html/Y2M/",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "jest": "^21.2.1",
    "mobile-detect": "^1.3.7"
  }
}
